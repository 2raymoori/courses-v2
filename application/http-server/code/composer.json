{
    "name": "galeas/galeas-api",
    "license": "Proprietary",
    "type": "project",
    "description": "Galeas API - depends on Symfony. nette/di is forced to version 2 because it breaks PHPStan's integration with Symfony.",
    "minimum-stability": "beta",
    "require": {
        "php": "^7.3.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alcaeus/mongo-php-adapter": "^1.1.3",
        "doctrine/dbal": "^2.6",
        "doctrine/mongodb": "^1.6",
        "doctrine/mongodb-odm": "dev-master#e4971557a8200456d87e2f1def8bde7fb07bbb3f",
        "doctrine/mongodb-odm-bundle": "dev-master#2b525ca25ba3bc0665073ddc9795ceaf95e6a263",
        "ocramius/proxy-manager": "2.2.*",
        "ekino/newrelic-bundle": "^2.0",
        "justinrainbow/json-schema": "^5.2",
        "symfony/console": "4.4.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "behat/behat": "^3.0.5",
        "friendsofphp/php-cs-fixer": "^2.12",
        "guzzlehttp/guzzle": "~6.0",
        "kwn/php-rdkafka-stubs": "^1.0.2",
        "nette/bootstrap": "^2.4",
        "nette/di": "^2.4.7",
        "pepakriz/phpstan-exception-rules": "^0.5",
        "phpstan/phpstan": "^0.11.1",
        "phpstan/phpstan-symfony": "^0.11",
        "phpunit/phpunit": "~6.0",
        "sensiolabs-de/deptrac-shim": "^0.4.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/doctrine/mongodb-odm.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/doctrine/DoctrineMongoDBBundle"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "Galeas\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Galeas\\Api\\": "tests/"
        }
    },
    "scripts": {},
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "provide": {
        "ext-mongo": "1.6.14"
    }
}
